    "components",
    "select",
    "selectbox",
    "szn",
    "seznam",
    "seznam.cz",
    "accessible",
    "accessibility",
    "a11y"
  ],
  "author": "Martin Jurča <martin.jurca@firma.seznam.cz>",
  "contributors": [
    {
      "name": "Lukáš Pazderka",
      "email": "lukas.pazderka@firma.seznam.cz"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seznam/szn-select/issues"
  },
  "homepage": "https://github.com/seznam/szn-select#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
